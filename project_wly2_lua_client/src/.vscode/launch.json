{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cocos2-launch",
            "type": "lua",
            "request": "launch",
            "runtimeType": "Cocos2",
            "localRoot": "${workspaceRoot}",
            "commandLine": "-workdir ${workspaceRoot}/../ -file src/main.lua",
            "port": 7003,
            "exePath": "E:\\project_wly2_lua_client\\simulator\\win32\\project_wly2_lua_client.exe",
            "printType": 1
        },
        {
            "name": "COCOS(remote debugging)",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Cocos2",
            "localRoot": "${workspaceRoot}",
            "port": 7003,
            "printType": 1
        },
        {
            "name": "Unity-ulua",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Unity",
            "localRoot": "${workspaceRoot}",
            "fileExtname": ".lua",
            "port": 7003,
            "printType": 1
        },
        {
            "name": "Unity-slua",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Unity",
            "localRoot": "${workspaceRoot}",
            "fileExtname": ".txt",
            "port": 7003,
            "printType": 1
        },
        {
            "name": "Unity-xlua",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Unity",
            "localRoot": "${workspaceRoot}",
            "fileExtname": ".txt",
            "port": 7003,
            "printType": 1
        },
        {
            "name": "LuaTest",
            "type": "lua",
            "request": "launch",
            "runtimeType": "LuaTest",
            "mainFile": "${fileBasenameNoExtension}",
            "localRoot": "${fileDirname}",
            "curFileExtname": "${fileExtname}",
            "fileExtname": ".lua",
            "port": 7003,
            "printType": 1
        }
    ]
}